#include <stdio.h>
int main()
{
    int arr[20],n,item,i;
    printf("enter the size of array:");
    scanf("%d",&n);
    printf("enter the new element to insert at begin:");
      scanf("%d",&item);
    for(int i=0;i<n;i++)
    scanf("%d",&arr[i]);
   arr[n]=item;
   for(int i=0;i<=n;i++)
 {
     printf("%d ",arr[i]);
 }
    return 0;
}
