#include <stdio.h>

int main()
{
    int arr[20],n,item;
    printf("enter the size of array:");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    n++;
     printf("enter the new element to insert at begin:");
      scanf("%d",&item);
 for(int i=n;i>1;i--)
 {
     arr[i-1]=arr[i-2];
 }
 arr[0]=item;
 for(int i=0;i<n;i++)
 {
     printf("%d ",arr[i]);
 }
    return 0;
}
