#include <stdio.h>

int main()
{ int arr[10],n,sum=0,count;
scanf("%d",&n);
int p=n+1;
   for(int i=0;i<n;i++)
   {
    scanf("%d",&arr[i]);
    sum=sum+arr[i];
   }
  int r=(p*(p+1)/2)-sum;
  printf("%d",r);
    return 0;
}



#include <stdio.h>

int main()
{ int arr[10]={10,20,30,50,60},a[]={10,20,30,40,50,60},s1=0,s2=0;

   for(int i=0;i<5;i++)
   {
    
    s1=s1+arr[i];
   
   }
   for(int i=0;i<6;i++)
   {
       s2=s2+a[i];
   }
  int r=s2-s1;
  printf("%d",r);
    return 0;
}
