#include <stdio.h>
#include<stdlib.h>
#define C 5
int queue[C];
int front=0,rear=0;
int ins(int item)
{
    if(C==rear)
        printf("queue is full");
    else
    {
        queue[rear]=item;
        rear--;
        printf("%d",item);
    }
}
int del()
{
    
    if(front==rear)
        printf("queue is empty");
    else
    {
       for(int i=front;i<rear;i++)
         queue[i]=queue[i+1];
    }
    rear--;
}

int trave()
{
    if(front==rear)
        printf("queue is empty");
    else
    {
      printf("Elements of the queue are:");
      for(int i=front;i<rear;i++)
      printf("%d ",queue[i]);
    }
}
int main()
{
    while(1)
    {
    printf("\n1.Inset\n");
     printf("2.Delete\n");
     printf("3.Travese\n");
     printf("4.Exit\n");
    int ch,item;
    printf("enter your choice:");
    scanf("%d",&ch);
    switch(ch)
    {
        case 1:printf("eneter the element:");
               scanf("%d",&item);
               ins(item);
               break;
        case 2:del();
               break;
        case 3:trave();
               break;
        case 4:
               return 0;
    }
    }
}
