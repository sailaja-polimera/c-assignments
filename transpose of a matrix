#include<stdio.h>
int main()
{
    int m[5][5],r=2,c=3;
    printf("Enter the matrix elememts:");
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            scanf("%d",&m[i][j]);
    }
}
  printf("The matrix elememts are:");
   printf("\n");
for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            printf("%d ",m[i][j]);
    }
    printf("\n");
}
printf("Transpose of a matrix is:");
printf("\n");
for(int i=0;i<c;i++)
    {
        for(int j=0;j<r;j++)
        {
            printf("%d ",m[j][i]);
    }
    printf("\n");
}
}
