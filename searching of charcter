#include <stdio.h>
# define LEN_ARR 5
int main()
{
   char arr[]={'r','a','g','h','u'};
   char temp;
   char *ptr=&temp;
   scanf("%c",&temp);
   for(int i=0;i<LEN_ARR;i++)
   
     {
       if(arr[i]==*ptr)
       {
       printf("value found at %d index",i);
       }
      
}
           
      
   
    return 0;
}
