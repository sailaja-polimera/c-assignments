#include <stdio.h>
#include <string.h>

int main () {
   char src[50] = "sailaja";
   char des[50]="polimera";
   printf("Before memcpy des = %s\n", des);
   memcpy(des, src,sizeof(src));
   printf("After memcpy dest = %s\n", des);
   return 0;
}

output:

Before memcpy des = polimera
After memcpy des = sailaja
